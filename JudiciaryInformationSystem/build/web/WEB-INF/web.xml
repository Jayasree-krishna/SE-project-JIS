<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UserLogIn</servlet-name>
        <servlet-class>JudiciaryServlet1.UserLogIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>JudiciaryServlet1.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterCase</servlet-name>
        <servlet-class>JudiciaryServlet1.RegisterCase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registrar</servlet-name>
        <servlet-class>JudiciaryServlet1.Registrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserManagement</servlet-name>
        <servlet-class>JudiciaryServlet1.UserManagement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CaseManagement</servlet-name>
        <servlet-class>JudiciaryServlet1.CaseManagement</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCaseRR</servlet-name>
        <servlet-class>JudiciaryServlet1.ViewCaseRR</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DefendantHome</servlet-name>
        <servlet-class>JudiciaryServlet1.DefendantHome</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdjournCase</servlet-name>
        <servlet-class>JudiciaryServlet1.AdjournCase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStatus</servlet-name>
        <servlet-class>JudiciaryServlet1.UpdateStatus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLogIn</servlet-name>
        <url-pattern>/UserLogIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterCase</servlet-name>
        <url-pattern>/RegisterCase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registrar</servlet-name>
        <url-pattern>/Registrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserManagement</servlet-name>
        <url-pattern>/UserManagement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CaseManagement</servlet-name>
        <url-pattern>/CaseManagement</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewCaseRR</servlet-name>
        <url-pattern>/ViewCaseRR</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DefendantHome</servlet-name>
        <url-pattern>/DefendantHome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdjournCase</servlet-name>
        <url-pattern>/AdjournCase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStatus</servlet-name>
        <url-pattern>/UpdateStatus</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
